##!/usr/bin/python3
'''
  Author:  Funing Jia
  Purpose: Using generators to generate prime numbers in any legal range.
  Created: 16/6/2020
'''
class PrimeNumbers:
    def __init__(self,start,end):
        self.start=start            #start为起始位置，end为终止位置
        self.end=end

    def PrimeNumbers(self,k):         #定义判断是否为素数的函数
        if k<2:
            return False
        for i in range(2,k):
            if k%i==0:
                return False
        return True

    def __iter__(self):             #从起始到终止不断迭代
        for k in range(self.start,self.end):
            if self.PrimeNumbers(k):
                print('数据范围从%d到%d' % (self.start, self.end))
                yield k

    for x in PrimeNumbers(1, 100):
        print(x)
